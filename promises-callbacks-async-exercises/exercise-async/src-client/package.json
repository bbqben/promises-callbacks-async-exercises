    "start": "npm run gulp:compile && npm-run-all --parallel gulp:watch json-server webpack:serve",
    "build": "npm run gulp:compile && npm run gulp:build && npm run webpack:build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gulp:build": "gulp build --gulpfile ./config/gulpfile.js",
