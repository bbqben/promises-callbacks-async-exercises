    "gulp:watch": "gulp watch --gulpfile ./config/gulpfile.js",
    "json-server": "node_modules/.bin/json-server ./config/fakedata.js --port 3601",
    "precommit": "lint-staged",
    "prettier": "node_modules/.bin/prettier --config ./config/.prettierrc --write \"app/**/*.*\" \"*.js\" \"*.json\" ",
    "webpack": "webpack",
    "webpack:build": "webpack --config ./config/webpack.config.js",
    "webpack:serve": "webpack-dev-server --config ./config/webpack.config.js --env.webpackMode development --open"
  },
  "lint-staged": {
